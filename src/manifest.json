{
  "name": "Remove Paywall Content",
  "description": "Remove Paywall Content from online news sites",
  "version": "0.1",
  "permissions": [
    "tabs","<all_urls>"
    ],
  "content_scripts": [
  {
    "matches": ["http://*/*"],
    "js": ["background.js"]
  }
  ],
  "browser_action": {
    "default_title": "Remove Paywall Content"
  },
  "manifest_version": 2
}
